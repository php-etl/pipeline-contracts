        }
    ],
    "require": {
        "php": "^8.2",
        "php-etl/bucket-contracts": "^0.2@dev"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5 || ^10.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.26.18",
        "rector/rector": "^0.15"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Contract\\Pipeline\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.4.x-dev",
            "dev-feature/qualityflow-improvments": "0.4.x-dev"
        }
